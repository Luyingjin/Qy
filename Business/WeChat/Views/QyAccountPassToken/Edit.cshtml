<div class="mini-toolbar" style="padding: 2px; border-bottom: 0;">
    <table style="width: 100%;">
        <tr>
            <td style="width: 100%; text-align: left">
                <a class="mini-button" plain="true" iconcls="icon-save" onclick="save">确定</a>
                <a class="mini-button" plain="true" iconcls="icon-cancel" onclick="closeWindow()">取消</a>
            </td>
            <td style="white-space: nowrap;">
                <a class="mini-button" onclick="showHelp()" iconcls="icon-help" plain="true">帮助</a>
            </td>
        </tr>
    </table>
</div>
<form id="dataForm" method="post">
    <input name="ID" class="mini-hidden" />
    <input name="QyID" class="mini-hidden" />
    <div class="basediv">
        <table width="100%" cellpadding="5" cellspacing="0" border="1" bordercolor="#333333"
            bordercolordark="#FFFFFF">
            <tr>
                <td width="15%">PassToken&nbsp;&nbsp;<b style="color: red;">*</b>
                </td>
                <td width="85%" colspan="3">
                    <input name="PassToken" class="mini-textbox" style="width: 100%;" required="true" vtype="maxLength:500;" />
                </td>
            </tr>
            <tr>
                <td>IP白名单&nbsp;&nbsp;<b style="color: red;">*</b>
                </td>
                <td colspan="3">
                    <input name="AllowIP" class="mini-textarea" style="width: 100%" vtype="maxLength:4000" />
                </td>
            </tr>
            <tr>
                <td>Oauth2白名单&nbsp;&nbsp;<b style="color: red;">*</b>
                </td>
                <td colspan="3">
                    <input name="OAuth2AllowDomain" class="mini-textarea" style="width: 100%" vtype="maxLength:4000" />
                </td>
            </tr>
        </table>
    </div>
</form>
